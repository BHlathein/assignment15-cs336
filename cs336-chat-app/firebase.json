{  
  "hosting": {  
   "public": "dist/cs336-chat-app",  
   "ignore": [  
    "firebase.json",  
    "**/.*",  
    "**/node_modules/**"  
   ],  
   "rewrites": [  
    {  
      "source": "**",  
      "destination": "/index.html"  
    }  
   ],  
   "headers": [  
    {  
      "source": "**/*.css",  
      "headers": [  
       {  
        "key": "Content-Type",  
        "value": "text/css"  
       }  
      ]  
    },  
    {  
      "source": "**/*.svg",  
      "headers": [  
       {  
        "key": "Content-Type",  
        "value": "image/svg+xml"  
       }  
      ]  
    }  
   ]  
  }  
}
